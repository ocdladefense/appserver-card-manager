{
	"comment": "This is a module template",
	"name": "Payment-profile-manager", 
	"description": "A module for building other modules.",
    "connectedApp":"default",
	"files": [
        "CustomerProfile.php",
        "PaymentProfile.php",
        "MerchantAuthentication.php"
    ],
	"routes": {
        "cards/show":{
            "callback":"showAllPaymentProfiles",
            "content-type":"text/html"
        },
        "card/create":{
            "callback":"create",
            "content-type":"text/html"
        },
        "card/edit/%id":{
            "callback":"edit",
            "content-type":"text/html"
        },
        "card/delete/%id":{
            "callback":"delete",
            "content-type":"text/html"
        }
    }
}

